{
  "name": "react-sketch-sample",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "react-sketch-sample",
    "manifest": "src/manifest.json",
    "main": "react-sketch-sample.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "author": "ikeda <ikd618@gmail.com>",
  "dependencies": {
    "chroma-js": "^1.3.7",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-primitives": "^0.6.0",
    "react-sketchapp": "^2.0.0",
    "react-test-renderer": "^16.4.1",
    "styled-components": "^3.3.3"
  }
}
